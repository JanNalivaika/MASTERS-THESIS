 journal = {International Journal of Precision Engineering and Manufacturing-Green Technology},
 doi = {10.1007/s40684-018-0058-x},
 file = {Pham, Ahn 2018 - High Precision Reducers for Industrial:Attachments/Pham, Ahn 2018 - High Precision Reducers for Industrial.pdf:application/pdf}
}


